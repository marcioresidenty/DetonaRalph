const element = document.getElementById('tab1');

function mmudarCorElemento() {
	  let pos = 0;
	  const intervalo = setInterval(() => {
	    pos += 10;
	    element.style.left = pos + 'px';
	    if (pos >= 300) {
	      clearInterval(interval);
	    }
	  }, 10);
	}

	moverElemento();
